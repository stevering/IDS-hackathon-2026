{
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "X-Frame-Options",
            "value": "ALLOW-FROM https://www.figma.com"
          },
          {
            "key": "Content-Security-Policy",
            "value": "frame-ancestors 'self' https://www.figma.com"
          }
        ]
      }
    ]
  }
  